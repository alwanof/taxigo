{
    "private": true,
    "scripts": {
        "dev": "npm run development",
        "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "watch": "npm run development -- --watch",
        "watch-poll": "npm run watch -- --watch-poll",
        "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --disable-host-check --config=node_modules/laravel-mix/setup/webpack.config.js",
        "prod": "npm run production",
        "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "build-drivers-map": "cd nova-components/DriversMap && npm run dev",
        "build-drivers-map-prod": "cd nova-components/DriversMap && npm run prod",
        "build-taxi-order": "cd nova-components/TaxiOrder && npm run dev",
        "build-taxi-order-prod": "cd nova-components/TaxiOrder && npm run prod",
        "build-OrderStream": "cd nova-components/OrderStream && npm run dev",
        "build-OrderStream-prod": "cd nova-components/OrderStream && npm run prod",
        "build-order-stream": "cd nova-components/OrderStream && npm run dev",
        "build-order-stream-prod": "cd nova-components/OrderStream && npm run prod",
        "build-orders-card": "cd nova-components/OrdersCard && npm run dev",
        "build-orders-card-prod": "cd nova-components/OrdersCard && npm run prod"
    },
    "devDependencies": {
        "axios": "^0.19",
        "bootstrap": "^4.0.0",
        "cross-env": "^7.0",
        "jquery": "^3.2",
        "laravel-mix": "^5.0.9",
        "lodash": "^4.17.19",
        "popper.js": "^1.12",
        "resolve-url-loader": "^2.3.1",
        "sass": "^1.20.1",
        "sass-loader": "^8.0.0",
        "vue": "^2.5.17",
        "vue-template-compiler": "^2.6.10"
    },
    "dependencies": {
        "parse": "^2.16.0",
        "vue2-google-maps": "^0.10.7"

    }
}
